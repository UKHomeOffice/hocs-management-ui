        "eslint-plugin-react": "^7.11.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^25.5.4",
        "jest-date-mock": "^1.0.8",
        "jest-enzyme": "^7.1.2",
        "lint-staged": "^7.3.0",
        "mini-css-extract-plugin": "^1.6.2",
        "nodemon": "^2.0.15",
        "postcss-loader": "^2.1.4",
        "pre-commit": "^1.2.2",
