        "@types/jest": "^24.0.18",
        "@types/react-dom": "^18.0.1",
        "@types/react-router-dom": "^4.3.4",
        "@types/react-select": "^3.0.4",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
        "autoprefixer": "^9.8.8",
