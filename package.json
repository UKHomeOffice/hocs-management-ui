        "webpack": "^4.42.1",
        "webpack-assets-manifest": "^3.1.0",
        "webpack-cli": "^3.1.2",
        "webpack-node-externals": "^1.7.2"
    },
    "pre-commit": [
        "lint-staged",
        "jest"
    ],
    "lint-staged": {
        "*.js": [
            "eslint ./ --ext js"
        ],
        "*.jsx": [
            "eslint ./ --ext jsx"
        ],
        "*.ts": [
            "eslint ./ --ext ts"
        ],
        "*.tsx": [
            "eslint ./ --ext tsx"
        ]
    },
    "husky": {
        "hooks": {
