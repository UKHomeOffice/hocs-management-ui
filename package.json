        "@testing-library/react": "^9.1.4",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^24.0.18",
        "@types/react-dom": "^16.8.4",
        "@types/react-router-dom": "^4.3.4",
        "@types/react-select": "^3.0.4",
        "autoprefixer": "^8.3.0",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.0.2",
        "babel-jest": "^25.2.4",
        "babel-loader": "^8.0.4",
        "core-js": "^3.2.1",
        "css-loader": "^3.2.0",
