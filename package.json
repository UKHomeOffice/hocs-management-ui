{
  "name": "hocs-management-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-dev": "webpack --mode=development --env.NODE_ENV=development",
    "build-prod": "webpack --mode=production --env.NODE_ENV=production",
    "test": "jest --coverage",
    "jest": "node_modules/jest/bin/jest.js  .",
    "lint:js": "eslint ./ --ignore-path .gitignore",
    "lint:jsx": "eslint ./ --ignore-path .gitignore",
    "lint:ts": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:tsx": "tslint -c tslint.json 'src/**/*.tsx'",
    "lint-staged": "node_modules/lint-staged/index.js",
    "lint": "npm run lint:js & npm run lint:jsx & npm run lint:ts & npm run lint:tsx"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.digital.homeoffice.gov.uk:2222/hocs/hocs-management-ui.git"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "\\.spec.utils.(js|jsx)$"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "server/**/*.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "autoprefixer": "^8.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.18.7",
    "postcss-loader": "^2.1.4",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.0.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.3",
    "webpack": "^4.20.2",
    "webpack-assets-manifest": "^3.1.0",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "pre-commit": [
    "lint-staged",
    "jest"
  ],
  "lint-staged": {
    "*.js": [
      "eslint ./ --fix --ignore-path .gitignore",
      "git add"
    ],
    "*.jsx": [
      "eslint ./ --fix --ignore-path .gitignore",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.335.0",
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "govuk-frontend": "^2.9.0",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-ga": "^2.5.6",
    "react-router-dom": "^4.3.1",
    "serialize-javascript": "^1.7.0",
    "uuid": "^3.3.2",
    "winston": "^2.4.2"
  }
}
