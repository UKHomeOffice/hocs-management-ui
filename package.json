{
    "name": "hocs-management-ui",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "build-dev": "webpack --mode=development --env.NODE_ENV=development",
        "build-prod": "webpack --mode=production --env.NODE_ENV=production",
        "test": "jest --coverage",
        "jest": "node_modules/jest/bin/jest.js  .",
        "lint:js": "eslint ./ --ignore-path .gitignore",
        "lint:jsx": "eslint ./ --ignore-path .gitignore",
        "lint:ts": "tslint -c tslint.json 'src/**/*.ts'",
        "lint:tsx": "tslint -c tslint.json 'src/**/*.tsx'",
        "lint-staged": "node_modules/lint-staged/index.js",
        "lint": "npm run lint:js & npm run lint:jsx & npm run lint:ts & npm run lint:tsx"
    },
    "repository": "github:UKHomeOffice/hocs-management-ui",
    "jest": {
        "setupFiles": [
            "./test/jestsetup.js"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "moduleNameMapper": {
            "\\.(css|less|scss|sass)$": "identity-obj-proxy"
        },
        "testPathIgnorePatterns": [
            "\\.spec.utils.(js|jsx)$",
            "/build/"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "server/**/*.js"
        ],
        "preset": "ts-jest",
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest",
            "^.+\\.(js|jsx)$": "babel-jest"
        }
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.8.4",
