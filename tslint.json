{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb"
    ],
    "rules": {
        "align": [false],
        "import-name": [false],
        "interface-name": [true, "never-prefix"],
        "max-line-length": [false],
        "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
        "ter-indent": [true, 4],
        "variable-name": [true, "allow-pascal-case"]
    },
    "rulesDirectory": []
}